[Unit]
Description=SetupOS config process
Before=generate-network-config.service
Before=setupos.service

[Service]
Type=oneshot
RemainAfterExit=true
ExecStart=/opt/ic/bin/output-wrapper.sh /dev/ttyS0 /opt/ic/bin/config create-setupos-config
StandardOutput=tty
StandardError=tty

[Install]
WantedBy=multi-user.target
